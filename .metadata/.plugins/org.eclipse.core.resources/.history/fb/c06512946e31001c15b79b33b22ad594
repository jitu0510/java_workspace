package com.ty.spring.springcore.test;

import org.springframework.context.ApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

public class ScopeTest {

	public static void main(String[] args) {
		ApplicationContext context = new ClassPathXmlApplicationContext("scope.xml");
		ScopeTest test = (ScopeTest) context.getBean("singletonclass");
		System.out.println(test);
		
		ScopeTest test1 = (ScopeTest) context.getBean("singletonclass");
		System.out.println(test1);

	}

	
}
