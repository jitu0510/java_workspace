package com.ty.spring.annotation.bean;

import java.io.Serializable;

import org.springframework.stereotype.Component;

import lombok.Data;
@Data
public class Address implements Serializable {
	
	public Address(String hno, String city) {
	
		this.hno = hno;
		this.city = city;
	}
	private String hno;
	private String city;

}
